# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Blind Bite' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Blind Bite
    pod 'Firebase'
    pod 'Firebase/Core'
    pod 'Firebase/Database'
    pod 'Firebase/Auth'
    pod ‘Google/SignIn’

  target 'Blind BiteTests' do
    inherit! :search_paths
    # Pods for testing
	pod ‘Firebase’
    pod ‘Firebase/Core’
    pod ‘Firebase/Database’
    pod ‘Firebase/Auth’
    pod ‘Google/SignIn’
  end

  target 'Blind BiteUITests' do
    inherit! :search_paths
    # Pods for testing
	pod ‘Firebase’
    pod ‘Firebase/Core’
    pod ‘Firebase/Database’
    pod ‘Firebase/Auth’
    pod ‘Google/SignIn’
  end

end
